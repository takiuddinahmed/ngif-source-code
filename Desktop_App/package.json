{
  "name": "image-detection",
  "version": "1.0.0",
  "description": "Detection of image",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . ngif --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=bugfixer --version-string.FileDescription=CE --version-string.ProductName=\"NGIF\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "create-installer-win": "node installers/windows/createinstaller.js"
  },
  "author": "Takiuddin Ahmed <takiuddinahmed@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "electron": "latest"
  },
  "dependencies": {}
}



  